{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "maicol07/account",
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": ">=8.5",
    "ext-fileinfo": "*",
    "ext-intl": "*",
    "ext-pdo": "*",
    "api-platform/laravel": "^4.2",
    "emargareten/inertia-modal": "^1.5",
    "fakerphp/faker": "^1.23",
    "inertiajs/inertia-laravel": "^2.0",
    "kiwilan/typescriptable-laravel": "^3.1",
    "laravel-lang/common": "^6.4",
    "laravel/fortify": "^1.23",
    "laravel/framework": "^12.0",
    "laravel/horizon": "^5.33",
    "laravel/octane": "^2.5",
    "laravel/pulse": "^1.2",
    "laravel/sanctum": "^4.0",
    "laravel/telescope": "^5",
    "laravel/tinker": "^2.9",
    "laravel/wayfinder": "^0.1.4",
    "maicol07/laravel-oidc-client": "^1.1",
    "marcreichel/igdb-laravel": "^5.3",
    "opcodesio/log-viewer": "^3.10",
    "phpdocumentor/type-resolver": "^1.12",
    "soved/laravel-gdpr": "^1.9",
    "spatie/laravel-activitylog": "^4.10"
  },
  "require-dev": {
    "amirami/localizator": "^0.14@alpha",
    "barryvdh/laravel-debugbar": "^3.13",
    "laravel/pail": "^1",
    "laravel/pint": "^1.13",
    "laravel/sail": "^1.26",
    "mockery/mockery": "^1.6",
    "nunomaduro/collision": "^8.0",
    "pestphp/pest": "^4.3",
    "pestphp/pest-plugin-laravel": "^4.0",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/",
      "Database\\Factories\\": "database/factories/",
      "Database\\Seeders\\": "database/seeders/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "setup": [
      "composer install",
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
      "@php artisan key:generate",
      "@php artisan migrate --force",
      "pnpm install",
      "pnpm run build"
    ],
    "dev": [
      "Composer\\Config::disableProcessTimeout",
      "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite --kill-others"
    ],
    "test": [
      "@php artisan config:clear --ansi",
      "@php artisan test"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-update-cmd": [
      "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
      "@php artisan lang:update"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi",
      "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
      "@php artisan migrate --graceful --ansi"
    ],
    "pre-package-uninstall": [
      "Illuminate\\Foundation\\ComposerScripts::prePackageUninstall"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
